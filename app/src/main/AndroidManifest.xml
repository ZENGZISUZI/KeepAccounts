<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zengsu.example.com.keepaccounts">

    <application
        android:name="zengsu.example.com.keepaccounts.entity.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">






        <!--android:name=".HomeActivity"-->
        <activity
            android:name="zengsu.example.com.keepaccounts.LaunchActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="zengsu.example.com.keepaccounts.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="zengsu.example.com.keepaccounts.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name="zengsu.example.com.keepaccounts.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="zengsu.example.com.keepaccounts.ListViewMultiChartActivity"
            android:parentActivityName="zengsu.example.com.keepaccounts.HomeActivity">
        </activity>
        <activity android:name="zengsu.example.com.keepaccounts.AboutActivity"
            android:parentActivityName="zengsu.example.com.keepaccounts.HomeActivity">
        </activity>


    </application>



    <!--xUtils框架需要的两个权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
</manifest>
